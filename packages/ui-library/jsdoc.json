{
  "source": {
      "include": [
        "./src"
      ],
      "includePattern": ".+\\.(js|jsx)$",
      "exclude": [
        "./src/demo/core",
        "./src/demo/css",
        "./src/demo/fonts",
        "./src/demo/images",
        "./src/demo/Demo.jsx",
        "./src/demo/highlight.js",
        "./src/css",
        "./src/fonts",
        "./src/images"
      ],
      "excludePattern": ".+/tests/.+$",
      "excludePattern": ".+/__mocks__/.+$"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./build-doc",
    "readme": "./README.md",
    "package": "./package.json",
    "recurse": true
  },
  "plugins": [
    "./node_modules/jsdoc-jsx",
    "plugins/markdown"
  ]
}
