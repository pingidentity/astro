{
  "name": "ui-library",
  "version": "2.2.0",
  "description": "A UI library made of reusable React components, util functions and static assets (e.g. CSS)",
  "author": "Ping Identity SaaS Interface Development",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://hg-od01.corp.pingidentity.com/r/gitweb?p=ui-library.git;a=tree;h=HEAD;hb=HEAD"
  },
  "dependencies": {
    "classnames": "1.2.1",
    "exif-js": "2.1.1",
    "es6-promise": "3.2.1",
    "fbjs": "0.4.0",
    "fix-orientation": "0.0.2",
    "isomorphic-fetch": "2.1.1",
    "moment": "2.11.1",
    "moment-range": "1.0.5",
    "object-assign": "4.0.1",
    "ping-react-color-picker": "3.1.3",
    "re-mutable": "1.1.1",
    "re-react": "1.1.5",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "superagent": "0.21.0",
    "underscore": "1.8.3",
    "underscore.string": "3.2.2"
  },
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.1",
    "babel-preset-react": "6.3.13",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "0.1.8",
    "clone": "1.0.2",
    "copy-webpack-plugin": "1.1.1",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.9.0",
    "http-server": "0.9.0",
    "jasmine": "2.4.1",
    "jest-cli": "0.8.2",
    "jsdoc": "3.3.3",
    "jsdoc-jsx": "0.1.0",
    "json-loader": "0.5.4",
    "lcov-parse": "0.0.10",
    "marked": "0.3.5",
    "node-sass": "3.4.2",
    "process": "0.11.2",
    "q": "1.4.1",
    "react": "0.14.6",
    "react-addons-test-utils": "0.14.6",
    "react-dom": "0.14.6",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.2",
    "redux": "3.0.5",
    "redux-thunk": "1.0.0",
    "reselect": "2.5.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "selenium-standalone": "5.1.0",
    "style-loader": "0.13.0",
    "stylelint-config-standard":"11.0.0",
    "stylelint": "7.0.2",
    "tty-table": "1.0.7",
    "url-loader": "0.5.7",
    "validator": "5.2.0 ",
    "wdio": "0.3.1",
    "wdio-jasmine-framework": "0.2.5",
    "wdio-junit-reporter": "0.1.0",
    "webdriverio": "4.0.9",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "peerDependencies": {
    "clone": "1.x",
    "reselect": "2.x",
    "react": "0.13 - 0.14",
    "react-dom": "0.14",
    "validator": "5.2.0 "
  },
  "engines": {
    "node": ">= 4.2.x",
    "npm": ">= 3.7.x"
  },
  "jest": {
    "rootDir": "./",
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine1",
    "scriptPreprocessor": "<rootDir>/jest/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/src/util/Utils.js",
      "<rootDir>/src/testutil/TestUtils.js",
      "<rootDir>/src/util/ReduxTestUtils.js",
      "<rootDir>/src/util/ReactWithDefaultMethods.js"
    ],
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node/",
      "/node_modules/",
      "/tests\/coverage/",
      "/build/",
      "/dist/",
      "/src/selenium/tests"
    ],
    "modulePathIgnorePatterns": [
      "/node/",
      "/build/",
      "/dist/"
    ]
  },
  "scripts": {
    "pack": "webpack --progress --colors --config webpack.config.js",
    "pack-landing": "webpack --progress --colors --config webpack.landing.config.js",
    "webpack-start-landing": "webpack-dev-server --content-base hosting/ -d --hot --port 8082 --host 0.0.0.0 --inline --progress --colors --config webpack.landing.config.js",
    "webpack-start": "webpack-dev-server -d --hot --host 0.0.0.0 --inline --progress --colors --config webpack.dev.config.js",
    "start": "npm run doc && npm run webpack-start",
    "doc": "jsdoc -c ./jsdoc.json",
    "jslint": "eslint --ext .js,.jsx .",
    "stylelint": "stylelint --syntax scss ./src/**/*.scss --config ./.stylelintrc",
    "jest": "jest && node ./clean-after.js",
    "coverage": "jest --coverage && node ./clean-after.js",
    "test": "jest",
    "verify": "npm run jslint && npm run test",
    "preintegration": "npm run clean-selenium-log & selenium-standalone install && selenium-standalone start -- -log ./selenium-standalone.log & http-server ./build > ./http-server.log 2>&1 & npm run pack",
    "integration": "wdio wdio.conf.js --suite $PATH_FILE",
    "postintegration": "npm run clean-processes",
    "clean-screenshots": "node ./clean-screenshot.js",
    "clean-selenium-log": "node ./clean-selenium-log.js",
    "clean-processes": "kill $(ps -ef | grep -E 'selenium|http-server' | awk '{print $2}')"
  },
  "browserify": {
    "transform": [[
        "babelify",
        {
            "presets": ["react"]
        }
    ]]
  }
}
