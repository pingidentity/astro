{
  "name": "ui-library",
  "version": "0.17.0-SNAPSHOT",
  "description": "A UI library made of reusable React components, util functions and static assets (e.g. CSS)",
  "author": "Ping Identity SaaS Interface Development",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://hg-od01.corp.pingidentity.com/r/gitweb?p=ui-library.git;a=tree;h=HEAD;hb=HEAD"
  },
  "dependencies": {
    "classnames": "1.2.1",
    "fbjs": "0.4.0",
    "isomorphic-fetch": "2.1.1",
    "moment": "2.11.1",
    "moment-range": "1.0.5",
    "object-assign": "4.0.1",
    "react-colorpicker": "0.3.14",
    "superagent": "0.21.0",
    "underscore": "1.8.3",
    "underscore.string": "3.2.2"
  },
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.8",
    "babel-jest": "6.0.1",
    "babel-loader": "6.2.1",
    "babel-preset-react": "6.3.13",
    "clean-webpack-plugin": "0.1.3",
    "css-loader": "0.16.0",
    "eslint": "1.6.0",
    "eslint-loader": "1.1.0",
    "eslint-plugin-react": "3.5.1",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "jest-cli": "0.4.19",
    "jsdoc": "3.3.3",
    "jsdoc-jsx": "0.1.0",
    "json-loader": "0.5.3",
    "node-sass": "3.3.3",
    "react-dnd": "1.1.8",
    "react-hot-loader": "1.3.0",
    "reactify": "1.1.1",
    "sass-loader": "3.0.0",
    "style-loader": "0.12.3",
    "url-loader": "0.5.6",
    "webpack": "1.12.2",
    "webpack-babel-jest": "1.0.2",
    "webpack-dev-server": "1.12.0"
  },
  "peerDependencies": {
    "react": "0.13.3",
    "redux": "3.0.0",
    "redux-thunk": "1.0.0"
  },
  "engines": {
    "node": ">= 0.10.x",
    "npm": ">= 2.7.x"
  },
  "jest": {
    "rootDir": "./",
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/src/testutil/TestUtils.js"
    ],
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node/",
      "<rootDir>/node_modules/",
      "<rootDir>/.*/tests/coverage/",
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ]
  },
  "scripts": {
    "build-do-not-use": "webpack --progress --colors --config webpack.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "test": "jest",
    "start": "npm run doc && npm run webpack-start",
    "webpack-start": "webpack-dev-server -d --hot --inline --progress --colors --config webpack.dev.config.js",
    "verify": "npm run lint && npm run test",
    "doc": "jsdoc -c ./jsdoc.json"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
