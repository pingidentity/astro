    "coverage-ci": "jest --coverage --ci && node ./clean-after.js",
    "test": "jest",
    "verify": "yarn run jslint && yarn run stylelint && yarn run coverage",
    "integration": "wdio wdio.conf.js",
    "postintegration": "yarn run clean-processes",
