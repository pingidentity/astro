{
  "name": "ui-library",
  "version": "0.19.0-SNAPSHOT",
  "description": "A UI library made of reusable React components, util functions and static assets (e.g. CSS)",
  "author": "Ping Identity SaaS Interface Development",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://hg-od01.corp.pingidentity.com/r/gitweb?p=ui-library.git;a=tree;h=HEAD;hb=HEAD"
  },
  "dependencies": {
    "classnames": "1.2.1",
    "exif-js": "2.1.1",
    "fbjs": "0.4.0",
    "fix-orientation": "0.0.2",
    "isomorphic-fetch": "2.1.1",
    "moment": "2.11.1",
    "moment-range": "1.0.5",
    "object-assign": "4.0.1",
    "react-color-picker": "3.1.0",
    "react-dnd": "1.1.8",
    "superagent": "0.21.0",
    "underscore": "1.8.3",
    "underscore.string": "3.2.2"
  },
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.1",
    "babel-preset-react": "6.3.13",
    "clean-webpack-plugin": "0.1.8",
    "copy-webpack-plugin": "1.1.1",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.9.0",
    "jest-cli": "0.8.2",
    "jsdoc": "3.3.3",
    "jsdoc-jsx": "0.1.0",
    "json-loader": "0.5.4",
    "marked": "0.3.5",
    "node-sass": "3.4.2",
    "process": "0.11.2",
    "react": "0.14.6",
    "react-addons-test-utils": "0.14.6",
    "react-dom": "0.14.6",
    "react-hot-loader": "1.3.0",
    "redux": "3.0.5",
    "redux-thunk": "1.0.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "peerDependencies": {
    "react": "0.13 - 0.14",
    "react-dom": "0.14",
    "redux": "3.0.x",
    "redux-thunk": "1.0.x"
  },
  "engines": {
    "node": ">= 4.2.x",
    "npm": ">= 3.7.x"
  },
  "jest": {
    "rootDir": "./",
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine1",
    "scriptPreprocessor": "<rootDir>/jest/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/src/testutil/TestUtils.js",
      "<rootDir>/src/util/ReduxTestUtils.js"
    ],
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node/",
      "/node_modules/",
      "/tests\/coverage/",
      "/build/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "/node/"
    ]
  },
  "scripts": {
    "pack": "npm run doc && webpack --progress --colors --config webpack.config.js",
    "webpack-start": "webpack-dev-server -d --hot --inline --progress --colors --config webpack.dev.config.js",
    "start": "npm run doc && npm run webpack-start",
    "doc": "jsdoc -c ./jsdoc.json",
    "lint": "eslint --ext .js,.jsx .",
    "jest": "jest && node ./clean-after.js",
    "coverage": "jest --coverage && node ./clean-after.js",
    "test": "jest",
    "verify": "npm run lint && npm run test"
  },
  "browserify": {
    "transform": [[
        "babelify",
        {
            "presets": ["react"]
        }
    ]]
  }
}
