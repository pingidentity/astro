{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "commonjs"
            },
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "transform-es2015-spread",
            {
                "loose": true
            }
        ],
        [
            "add-module-exports",
            {
                "addDefaultProperty": true
            }
        ],
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 3
            }
        ]
    ]
}
