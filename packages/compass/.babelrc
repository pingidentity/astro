{
    "presets": [

        [
            "@babel/preset-env",
            {
                "modules": "auto"
            }
        ],
        "@babel/preset-react",
        "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
        "transform-node-env-inline",
        "emotion",
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        [
            "transform-es2015-spread",
            {
                "loose": true
            }
        ],
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 3
            }
        ]
    ]
}
